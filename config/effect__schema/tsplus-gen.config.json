{
  "project": {
    "packageName": "@effect/schema",
    "rootDir": "src",
    "tsconfig": "tsconfig.build.json",
    "exclude": ["**/internal/**"],
    "moduleConfig": {
      "@effect/schema": {
        "staticPrefixes": ["make", "from", "unsafeFrom"],
        "fluentNamespaces": ["effect/schema"]
      }
    }
  },
  "namespaces": [
    {
      "name": "effect/schema",
      "exclude": ["@effect/schema/ParseResult#IO"],
      "type": {
        "include": true,
        "includeCompanion": true,
        "companionSuffix": ".Ops"
      },
      "static": {
        "include": true,
        "staticSuffix": ".Ops"
      },
      "fluent": {
        "include": true,
        "includeStatic": true,
        "staticSuffix": ".Aspects"
      },
      "pipeable": {
        "include": true,
        "includeStatic": true,
        "staticSuffix": ".Aspects"
      },
      "getter": {
        "include": true,
        "includeStatic": true,
        "staticSuffix": ".Aspects"
      }
    }
  ],
  "additionalExtensions": [
    ["effect/schema/Parser#decode", "getter", "decode", "effect/schema/Schema"],
    [
      "effect/schema/Parser#decodeEffect",
      "getter",
      "decodeEffect",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#decodeEither",
      "getter",
      "decodeEither",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#decodeOption",
      "getter",
      "decodeOption",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#decodePromise",
      "getter",
      "decodePromise",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#decodeResult",
      "getter",
      "decodeResult",
      "effect/schema/Schema"
    ],

    ["effect/schema/Parser#encode", "getter", "encode", "effect/schema/Schema"],
    [
      "effect/schema/Parser#encodeEffect",
      "getter",
      "encodeEffect",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#encodeEither",
      "getter",
      "encodeEither",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#encodeOption",
      "getter",
      "encodeOption",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#encodePromise",
      "getter",
      "encodePromise",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#encodeResult",
      "getter",
      "encodeResult",
      "effect/schema/Schema"
    ],

    ["effect/schema/Parser#is", "getter", "guard", "effect/schema/Schema"],

    ["effect/schema/Parser#parse", "getter", "parse", "effect/schema/Schema"],
    [
      "effect/schema/Parser#parseEffect",
      "getter",
      "parseEffect",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#parseEither",
      "getter",
      "parseEither",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#parseOption",
      "getter",
      "parseOption",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#parsePromise",
      "getter",
      "parsePromise",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#parseResult",
      "getter",
      "parseResult",
      "effect/schema/Schema"
    ],

    [
      "effect/schema/Parser#validate",
      "getter",
      "validate",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#validateEffect",
      "getter",
      "validateEffect",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#validateEither",
      "getter",
      "validateEither",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#validateOption",
      "getter",
      "validateOption",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#validatePromise",
      "getter",
      "validatePromise",
      "effect/schema/Schema"
    ],
    [
      "effect/schema/Parser#validateResult",
      "getter",
      "validateResult",
      "effect/schema/Schema"
    ]
  ]
}
